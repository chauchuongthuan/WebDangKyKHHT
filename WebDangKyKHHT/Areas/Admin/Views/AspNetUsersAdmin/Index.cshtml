@model IEnumerable<WebDangKyKHHT.Models.AspNetUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/DKKHHTAdmin.cshtml";
}
<div class="site-section">
    

    <div class="container">
        <section class="row">
            <div class="col-2 col-lg-10">
                <h2>THÔNG TIN SINH VIÊN</h2>
            </div>
            <br />
            <div class="col-10 col-lg-2">
                <button type="submit" class="btn btn-light-info ">@Html.ActionLink("Tạo mới", "Create")</button>
            </div>

        </section>
        <br />
        <div class="col-12">
            <table class="table custom-table">
                <tr>
                    <th>
                        Tên Sinh Viên
                    </th>
                    <th>
                        Email Trường
                    </th>

                    <th>
                        Số điện thoại
                    </th>
                    <th>
                        Xác nhận số điện thoại
                    </th>
                    <th>
                        Email cá nhân
                    </th>

                    <th>
                        Xác nhận Email
                    </th>
                    <th>
                        Ngày Sinh
                    </th>
                    @*<th>
                             Địa Chỉ
                        </th>
                        <th>
                           Mã Số Sinh viên
                        </th>*@
                    <th>
                    </th>

                </tr>

                @foreach (var item in Model)
                {

                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Ten)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PhoneNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PhoneNumberConfirmed)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UserName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EmailConfirmed)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NgaySinh)
                        </td>
                        @*<td>
                                @Html.DisplayFor(modelItem => item.DiaChi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.MSSV)
                            </td>*@
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>