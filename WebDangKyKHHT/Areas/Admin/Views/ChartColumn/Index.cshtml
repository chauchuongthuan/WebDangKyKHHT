
<html>
<head>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
        google.charts.load("current", { packages: ["corechart"] });
        google.charts.setOnLoadCallback(drawChart);
        function drawChart() {
            $(function () {
            $.ajax({
                type: 'POST',
                url: '@Url.Action("ChartC", "ChartColumn")',
                success: function (charts) {
                    var soluong = charts.dbchart1;
                    var data = google.visualization.arrayToDataTable([
                        ['Task', 'Hours per Day'],
                        ['Đã đăng kí', soluong],
                        ['Chưa đăng kí', 300 - soluong],
                    ]);
                    var options = {
                        title: 'Thống kê sinh viên',
                        is3D: true,
                    };
                    var chart = new google.visualization.PieChart(document.getElementById('piechart_3d'));
                    chart.draw(data, options);
                },
            });
            });
        }
    </script>


    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
        google.charts.load("current", { packages: ["corechart"] });
        google.charts.setOnLoadCallback(drawChart1);
        function drawChart1() {
            $(function () {
            $.ajax({
                type: 'POST',
                url: '@Url.Action("ChartC1", "ChartColumn")',
                success: function (charts1) {
                    var soluong1 = charts1.dbchart2;
                    var data1 = google.visualization.arrayToDataTable([
                        ['Task1', 'Hours per Day1'],
                        ['Đã đăng kí', soluong1],
                        ['Chưa đăng kí', 300 - soluong1],
                    ]);
                    var options = {
                        title: 'Thống kê sinh viên1',
                        is3D: true,
                    };
                    var chart = new google.visualization.PieChart(document.getElementById('piechart_3d1'));
                    chart.draw(data1, options);
                },
            });
            });
        }
    </script>

</head>
<body>
    <div id="piechart_3d" style="width: 900px; height: 500px;"></div>
    <div id="piechart_3d1" style="width: 900px; height: 500px;"></div>
</body>
</html>